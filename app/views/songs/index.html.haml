%center
  = select("genre", "name", Genre.all.collect { |g| [g.name, g.id] }, {include_blank: 'genre'})

  = select("feeling", "name", Feeling.all.collect {|f| [f.name, f.id] }, {include_blank: 'feeling'})

%br

- @song_frames.each do |song, id|
  = raw song
  - if current_user
    .left
      = form_tag do
        = text_field_tag "feeling-#{id}", nil, {placeholder: 'how does this song feel?', required: true}
        = submit_tag 'tag'
    .right
      .star-off
    %br
  %br
  %br
  %br
  %br

